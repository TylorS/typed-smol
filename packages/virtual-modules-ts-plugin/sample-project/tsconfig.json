{
  "extends": "@typed/tsconfig/base",
  "include": [
    "entry.ts",
    "router-demo.ts",
    "server.ts",
    "routes/**/*.ts",
    "api/**/*.ts",
    "typed-vite-dev-server.d.ts"
  ],
  "exclude": [
    "routes/**/*.example.ts"
  ],
  "compilerOptions": {
    "noEmit": true,
    "types": [
      "node",
      "vite/client"
    ],
    "plugins": [
      {
        "name": "@typed/virtual-modules-ts-plugin"
      }
    ]
  }
}