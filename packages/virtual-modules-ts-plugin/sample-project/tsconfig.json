{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "skipLibCheck": true,
    "plugins": [
      {
        "name": "@typed/virtual-modules-ts-plugin",
        "plugins": ["./plugin.cjs", "./plugins/router-plugin.cjs"],
        "debounceMs": 50
      }
    ]
  },
  "include": ["entry.ts", "router-demo.ts", "routes/**/*.ts"],
  "exclude": ["routes/**/*.example.ts"]
}
