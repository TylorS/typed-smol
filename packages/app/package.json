{
  "name": "@typed/app",
  "version": "1.0.0-beta.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "[ -d dist ] || rm -f tsconfig.tsbuildinfo; tsc",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@typed/id": "workspace:*",
    "@typed/router": "workspace:*",
    "@typed/virtual-modules": "workspace:*",
    "effect": "catalog:",
    "@typed/fx": "workspace:*",
    "@typed/navigation": "workspace:*",
    "@typed/template": "workspace:*",
    "@effect/platform-node": "catalog:"
  },
  "devDependencies": {
    "@effect/platform-node": "catalog:",
    "@typed/fx": "workspace:*",
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.0",
    "effect": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "vite": "^7.3.1"
  },
  "peerDependencies": {
    "typescript": "catalog:",
    "vite": ">=5.0.0"
  },
  "files": [
    "dist",
    "src"
  ]
}