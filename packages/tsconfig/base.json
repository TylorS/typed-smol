{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2024",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "plugins": [
      {
        "name": "@typed/virtual-modules-ts-plugin",
        "debounceMs": 50
      }
    ]
  },
  "exclude": ["node_modules", "dist"]
}
