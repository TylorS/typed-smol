{
  "name": "typed-smol",
  "private": true,
  "scripts": {
    "build": "pnpm -r build && tsc -b tsconfig.build.json && pnpm --filter @typed/virtual-modules-ts-plugin run build:plugins",
    "lint": "oxlint",
    "format": "oxfmt",
    "test": "pnpm -r run test",
    "typed": "npx tsx ./packages/cli/src/bin.ts"
  },
  "devDependencies": {
    "@typed/virtual-modules-ts-plugin": "workspace:*",
    "oxfmt": "catalog:",
    "oxlint": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "packageManager": "pnpm@10.0.0",
  "pnpm": {
    "overrides": {
      "effect": "4.0.0-beta.14"
    }
  }
}
